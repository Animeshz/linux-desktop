{
    "debug.console.fontFamily": "CaskaydiaCove Nerd Font Mono",
    "editor.fontFamily": "'CaskaydiaCove Nerd Font Mono', 'Droid Sans Mono', 'monospace', monospace",
    "editor.fontLigatures": true,
    "workbench.iconTheme": "eq-material-theme-icons-light",
    "workbench.colorTheme": "Community Material Theme Palenight",
    "vscode-neovim.neovimExecutablePaths.linux": "/usr/bin/nvim",
    "vscode-neovim.mouseSelectionStartVisualMode": false,
    "nix.enableLanguageServer": true,
    "[nix]": {
        "editor.defaultFormatter": "brettm12345.nixfmt-vscode"
    },
    "nixfmt.path": "nixfmt -w=120",
    "remote.SSH.useLocalServer": false,
    "[awk]": {
        // use id:luggage66.awk extension for highlighting
        "editor.semanticHighlighting.enabled": false
    },
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "[vue]": {
        "editor.defaultFormatter": "Vue.volar"
    },
    "explorer.confirmDragAndDrop": false,
    "window.zoomLevel": -1,
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[css]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "latex-workshop.latex.build.forceRecipeUsage": false,
    "puppet.installType": "agent"
}